  "Intervention": {
    "intervention_strategies": {
      "resistance_management": {
        "description": "Approaches for working with client resistance/discord",
        "core_principles": [
          "Roll with resistance rather than opposing it",
          "Avoid argumentation",
          "Redirect momentum",
          "Explore rather than correct"
        ],
        "specific_techniques": {
          "simple_reflection": {
            "description": "Straightforward reflection of resistant statement",
            "purpose": "Demonstrate understanding without reinforcing",
            "examples": [
              {
                "client": "You can't tell me what to do.",
                "practitioner": "You want to make your own decisions.",
                "notes": "Captures core meaning without confrontation"
              }
            ]
          },
          "amplified_reflection": {
            "description": "Reflection that slightly exaggerates resistance",
            "purpose": "Create space for client to argue other side",
            "examples": [
              {
                "client": "I don't really drink that much.",
                "practitioner": "There's absolutely nothing about your drinking that concerns you.",
                "notes": "Slight exaggeration invites moderation"
              }
            ]
          },
          "double_sided_reflection": {
            "description": "Reflection incorporating both sides of ambivalence",
            "purpose": "Acknowledge resistance while highlighting change talk",
            "examples": [
              {
                "client": "I know I should quit but I really enjoy smoking.",
                "practitioner": "You enjoy smoking AND you have some concerns about it.",
                "notes": "Captures both sides with emphasis on change aspect"
              }
            ]
          },
          "shifting_focus": {
            "description": "Temporarily moving away from resistant topic",
            "purpose": "Reduce opposition by changing subject",
            "examples": [
              {
                "situation": "Strong resistance to discussing alcohol use",
                "response": "What other concerns would you like to discuss today?",
                "notes": "Allows productive conversation to continue"
              }
            ]
          },
          "reframing": {
            "description": "Offering alternative interpretation",
            "purpose": "Present new perspective on situation",
            "examples": [
              {
                "client": "I've tried to quit so many times and failed.",
                "practitioner": "You're persistent in working on this challenge.",
                "notes": "Reframes failure as determination"
              }
            ]
          },
          "emphasizing_autonomy": {
            "description": "Explicitly acknowledging client choice/control",
            "purpose": "Reduce resistance by removing pressure",
            "examples": [
              {
                "client": "Everyone's trying to make me quit.",
                "practitioner": "This is completely your choice to make.",
                "notes": "Removes external pressure"
              }
            ]
          }
        }
      },
      "change_talk_cultivation": {
        "description": "Strategies for evoking and strengthening change talk",
        "evocation_techniques": {
          "importance_rulers": {
            "description": "Numerical scaling questions about importance of change",
            "format": "Scale of 0-10 questions",
            "key_questions": [
              "How important is it to you to make this change?",
              "Why are you at X and not lower?",
              "What would it take to get to a higher number?"
            ]
          },
          "confidence_rulers": {
            "description": "Numerical scaling questions about confidence in ability to change",
            "format": "Scale of 0-10 questions",
            "key_questions": [
              "How confident are you that you could make this change?",
              "What makes you a X and not lower?",
              "What would help you feel more confident?"
            ]
          },
          "looking_forward": {
            "description": "Questions about future scenarios",
            "examples": [
              "What would be different if you made this change?",
              "How would you like things to be in 5 years?",
              "What concerns you about continuing as things are?"
            ]
          },
          "looking_back": {
            "description": "Questions about past experiences",
            "examples": [
              "What was different before this became a concern?",
              "Tell me about previous successes with change.",
              "What have you learned from past attempts?"
            ]
          }
        }
      }
    }
  },
  "MI_Spirit_process": {
    "mi_spirit": {
      "core_elements": {
        "partnership": {
          "description": "Collaborative relationship between practitioner and client",
          "key_features": [
            "Power sharing",
            "Mutual exploration",
            "Avoidance of expert role",
            "Client as expert on self"
          ],
          "behavioral_markers": {
            "positive_indicators": [
              "Asking permission before giving advice",
              "Using inclusive language (we, let's explore)",
              "Acknowledging client expertise",
              "Offering choices and options"
            ],
            "negative_indicators": [
              "Assuming authority position",
              "Giving unsolicited advice",
              "Dismissing client views",
              "Taking expert stance"
            ]
          }
        },
        "acceptance": {
          "description": "Fundamental respect for client worth and potential",
          "components": {
            "absolute_worth": {
              "description": "Unconditional positive regard for client",
              "examples": [
                "Maintaining respect despite disagreement",
                "Avoiding judgment of choices",
                "Expressing genuine care"
              ]
            },
            "accurate_empathy": {
              "description": "Active interest in understanding client's perspective",
              "indicators": [
                "Careful listening",
                "Checking understanding",
                "Showing genuine curiosity",
                "Reflecting deeper meaning"
              ]
            },
            "autonomy_support": {
              "description": "Honoring client's right and capacity for self-direction",
              "examples": [
                "Emphasizing personal choice",
                "Respecting decisions",
                "Supporting self-determination"
              ]
            },
            "affirmation": {
              "description": "Acknowledging client strengths and efforts",
              "examples": [
                "Noticing positive steps",
                "Highlighting capabilities",
                "Recognizing good intentions"
              ]
            }
          }
        },
        "evocation": {
          "description": "Drawing out rather than imposing ideas",
          "key_principles": [
            "Resources and motivation reside within client",
            "Client insights are more powerful than practitioner insights",
            "Drawing out is more effective than putting in",
            "Client is source of solutions"
          ],
          "techniques": {
            "asking_vs_telling": {
              "description": "Preference for eliciting over educating",
              "examples": [
                "What do you already know about...?",
                "What ideas do you have about...?",
                "What has worked for you before?"
              ]
            },
            "exploring_motivations": {
              "description": "Helping client articulate own reasons for change",
              "methods": [
                "Looking forward questions",
                "Values exploration",
                "Goal clarification",
                "Past success examination"
              ]
            }
          }
        },
        "compassion": {
          "description": "Active promotion of client welfare",
          "manifestations": {
            "verbal": [
              "Expressions of genuine concern",
              "Acknowledgment of difficulty",
              "Support during struggles",
              "Celebration of successes"
            ],
            "behavioral": [
              "Maintaining focus on client benefit",
              "Prioritizing client welfare",
              "Supporting client goals",
              "Demonstrating patience"
            ],
            "attitudinal": [
              "Genuine care for outcomes",
              "Desire for client success",
              "Commitment to helping",
              "Acceptance of pace"
            ]
          }
        }
      },
      "processes": {
        "engaging": {
          "description": "Establishing working relationship and alliance",
          "key_strategies": {
            "rapport_building": {
              "techniques": [
                "Using appropriate open questions",
                "Demonstrating genuine interest",
                "Finding common ground",
                "Showing respect for client experience"
              ]
            },
            "alliance_formation": {
              "elements": [
                "Goal agreement",
                "Task agreement",
                "Emotional bond",
                "Role clarity"
              ]
            }
          },
          "indicators": {
            "positive": [
              "Client openness",
              "Active participation",
              "Emotional sharing",
              "Trust demonstrations"
            ],
            "negative": [
              "Guardedness",
              "Minimal responses",
              "Hostility",
              "Disengagement"
            ]
          }
        },
        "focusing": {
