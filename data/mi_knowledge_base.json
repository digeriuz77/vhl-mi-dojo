  "change_talk_resistance": {
    "change_talk_dynamics": {
      "types": {
        "preparatory_change_talk": {
          "desire": {
            "description": "Statements expressing wanting, wishing, or hoping for change",
            "examples": [
              {
                "client_statement": "I wish I could get control of this.",
                "strength": "moderate",
                "notes": "Expresses desire without commitment"
              }
            ],
            "recognition_features": [
              "Want statements",
              "Wish statements",
              "Hope expressions",
              "Liking/disliking current situation"
            ]
          },
          "ability": {
            "description": "Statements about capability to change",
            "examples": [
              {
                "client_statement": "I could probably cut down if I really tried.",
                "strength": "moderate",
                "notes": "Acknowledges capability with qualification"
              }
            ],
            "recognition_features": [
              "Can statements",
              "Could statements",
              "Ability expressions",
              "Past success references"
            ]
          },
          "reasons": {
            "description": "Specific arguments for change",
            "examples": [
              {
                "client_statement": "My blood pressure would probably go down if I quit.",
                "strength": "high",
                "notes": "Specific health benefit identified"
              }
            ],
            "recognition_features": [
              "Because statements",
              "If-then statements",
              "Benefit identification",
              "Problem recognition"
            ]
          },
          "need": {
            "description": "Expressions of necessity/urgency",
            "examples": [
              {
                "client_statement": "I've got to do something about this.",
                "strength": "high",
                "notes": "Clear expression of necessity"
              }
            ],
            "recognition_features": [
              "Need statements",
              "Must statements",
              "Have to expressions",
              "Urgency indicators"
            ]
          }
        },
        "commitment_language": {
          "description": "Statements indicating decision/intention to change",
          "strength_levels": {
            "weak": {
              "examples": [
                "I'll think about it",
                "Maybe I'll try",
                "I might do that"
              ],
              "features": [
                "Tentative language",
                "Future focus",
                "Qualifiers present"
              ]
            },
            "moderate": {
              "examples": [
                "I'm planning to",
                "I intend to",
                "I'll probably"
              ],
              "features": [
                "More definite language",
                "Near-future focus",
                "Some qualification"
              ]
            },
            "strong": {
              "examples": [
                "I will",
                "I'm going to",
                "I've decided to"
              ],
              "features": [
                "Definite language",
                "Present/immediate focus",
                "Clear commitment"
              ]
            }
          }
        }
      },
      "sustain_talk": {
        "description": "Language favoring status quo or against change",
        "categories": {
          "desire_to_maintain": {
            "examples": [
              "I like drinking",
              "I don't want to quit"
            ]
          },
          "inability": {
            "examples": [
              "I can't do it",
              "It's too hard"
            ]
          },
          "reasons_against": {
            "examples": [
              "It helps me relax",
              "I need it to cope"
            ]
          },
          "need_to_maintain": {
            "examples": [
              "I have to keep drinking",
              "I can't live without it"
            ]
          }
        }
      }
    }
  },
  "core_response": {
    "metadata": {
      "description": "Comprehensive classification of motivational interviewing responses and patterns",
      "version": "2.0",
      "usage": "Training and pattern recognition for MI practitioners",
      "key_source": "MINT Training Manual 2014"
    },
    "core_response_types": {
      "reflective_listening": {
        "description": "Practitioner statements that capture and express understanding of client meaning",
        "levels": {
          "simple_reflection": {
            "description": "Repeating or slightly rephrasing client's statements",
            "key_features": [
              "Stays close to client's original words",
              "Maintains same level of meaning",
              "Often uses similar vocabulary",
              "May slightly rearrange sentence structure"
            ],
            "examples": [
              {
                "client": "I don't think I drink that much really.",
                "practitioner": "You don't think your drinking is excessive.",
                "notes": "Maintains core meaning with slight rephrasing"
              },
              {
                "client": "The blackouts scare me.",
                "practitioner": "You're frightened by the blackouts.",
                "notes": "Simple emotion reflection"
              }
            ],
            "timing_guidance": [
              "Most appropriate early in conversation",
              "Useful when building initial rapport",
              "Helpful when meaning is unclear",
              "Good foundation before moving to complex reflections"
            ]
          },
          "complex_reflection": {
            "description": "Adds substantial meaning or makes inferences beyond client's stated words",
            "key_features": [
              "Captures underlying meaning",
              "Makes informed guesses about deeper significance",
              "Often uses different vocabulary",
              "May connect different elements client has shared"
            ],
            "subtypes": {
              "amplified_reflection": {
                "description": "Overstates or exaggerates client's meaning to elicit clarification",
                "examples": [
                  {
                    "client": "I probably drink too much sometimes.",
                    "practitioner": "So you're completely out of control with alcohol.",
                    "notes": "Intentional overstatement to allow client correction"
                  }
                ]
              },
              "double_sided_reflection": {
                "description": "Captures both sides of client's ambivalence",
                "examples": [
                  {
                    "client": "I know I should quit smoking but I really enjoy it.",
                    "practitioner": "On one hand smoking gives you pleasure, and at the same time you have concerns about it.",
                    "notes": "Acknowledges both motivation and barriers"
                  }
                ]
              },
              "continuing_the_paragraph": {
                "description": "Anticipates next logical element of client's narrative",
                "examples": [
                  {
                    "client": "Every morning I wake up feeling awful.",
                    "practitioner": "And you're wondering if that's connected to your drinking.",
                    "notes": "Makes logical inference about connection"
                  }
                ]
              }
            },
            "timing_guidance": [
              "More appropriate after building rapport",
              "Use when meaning becomes clearer",
              "Particularly useful for highlighting change talk",
              "Best employed once trust is established"
            ]
          }
        }
      },
      "questions": {
        "open_questions": {
          "description": "Questions that invite elaboration and cannot be answered with yes/no",
          "key_features": [
            "Begin with how, what, why, tell me about",
            "Allow client to direct response content",
            "Encourage deeper exploration",
            "Support client autonomy"
          ],
          "categories": {
            "evocative_questions": {
              "description": "Questions specifically designed to elicit change talk",
              "examples": [
                {
                  "question": "What concerns you about your current situation?",
                  "purpose": "Elicit problem recognition"
                },
                {
                  "question": "How would you like things to be different?",
                  "purpose": "Elicit desire for change"
                },
                {
                  "question": "What are the most important reasons you might want to make this change?",
                  "purpose": "Elicit motivation"
                }
              ]
            },
            "elaboration_questions": {
              "description": "Questions that invite deeper exploration of a topic",
              "examples": [
                {
                  "question": "Tell me more about that.",
                  "purpose": "General elaboration"
                },
                {
                  "question": "What else have you noticed?",
                  "purpose": "Broaden exploration"
                }
              ]
            }
          }
        },
        "closed_questions": {
          "description": "Questions with limited response options, often yes/no",
          "appropriate_uses": [
            "Gathering specific information",
            "Clarifying understanding",
            "Focusing conversation"
          ],
          "cautions": [
            "Can lead to question-answer trap",
            "May limit client exploration",
            "Can shift focus from client to practitioner agenda"
          ]
        }
      }
    }
  },
  "exploring_ambivalance": {
    "ambivalence_exploration": {
      "description": "Systematic exploration of client ambivalence through structured dialogue",
      "change_status_categories": {
        "status_quo": {
          "description": "Exploring current behavior maintenance",
          "exploration_areas": {
            "benefits": {
              "description": "Exploring positive aspects of not changing",
              "example_sequence": {
                "practitioner_prompt": "So tell me what's good about not exercising?",
                "typical_responses": [
                  "No energy required",
                  "Don't have to make time",
                  "Can stay comfortable",
                  "No pressure or expectations"
                ],
                "practitioner_strategies": [
                  "Use open questions to explore benefits",
                  "Reflect understanding without reinforcing",
                  "Listen for embedded change talk",
                  "Note ambivalence markers"
                ]
              }
            },
            "costs": {
              "description": "Exploring negative aspects of not changing",
              "example_sequence": {
                "initial_prompt": "Tell me what you dislike about not exercising?",
                "follow_up_strategies": [
                  {
                    "prompt": "Anything else?",
                    "purpose": "Deepen exploration of costs"
                  },
                  {
                    "prompt": "You sound pretty tired and overwhelmed already. What else?",
                    "purpose": "Connect current symptoms to behavior"
                  }
                ],
                "typical_themes": [
                  "Health consequences",
                  "Emotional costs",
                  "Lost benefits of previous change",
                  "Identity conflicts"
                ]
              }
            }
          }
        },
        "change_consideration": {
          "description": "Exploring potential behavior change",
          "exploration_areas": {
            "barriers": {
              "description": "Exploring challenges of making change",
              "example_sequence": {
                "practitioner_prompt": "Imagine you did decide to start. What would be hard, get in the way?",
                "common_barriers": [
                  "Current fitness level",
                  "Social discomfort",
                  "Time management",
                  "Self-image concerns"
                ],
                "practitioner_responses": {
                  "reflection_examples": [
                    "So it would be hard to call old running friends because they'd look down on you?",
                    "You mentioned time as a problem"
                  ],
                  "purpose": "Acknowledge barriers while listening for change talk"
                }
              }
            },
            "benefits": {
              "description": "Exploring positive aspects of making change",
              "example_sequence": {
                "practitioner_prompt": "So if you did start and kept it up, what would that be like?",
                "follow_up_strategies": [
                  {
                    "prompt": "Anything else?",
                    "purpose": "Gather additional benefits"
                  },
                  {
                    "prompt": "It felt good physically. Do you remember what your mood was like when you were exercising?",
                    "purpose": "Explore additional benefit domains"
                  }
                ],
                "benefit_categories": {
                  "physical": [
                    "More energy",
                    "Better fitness",
                    "Weight management"
                  ],
                  "emotional": [
                    "Improved mood",
                    "Better self-image",
                    "Increased confidence"
                  ],
                  "lifestyle": [
                    "More active social life",
                    "Broader range of activities",
                    "Increased engagement"
                  ]
                }
              }
            }
          }
        }
      },
      "practitioner_guidance": {
        "sequence_management": {
          "description": "Strategic management of exploration sequence",
          "key_principles": [
            "Start with current behavior exploration",
            "Balance discussion of pros and cons",
            "Listen for natural transition points",
            "Follow client's energy and engagement"
          ],
          "transition_strategies": {
            "status_quo_to_change": {
              "markers": [
                "Expression of dissatisfaction",
                "Reference to past success",
                "Mention of change possibilities"
              ],
              "example_transitions": [
                "You mentioned feeling differently when you were exercising...",
                "Tell me more about when you were running before..."
              ]
            }
          }
        },
        "response_patterns": {
          "sustain_talk_management": {
            "example_sequence": [
              {
                "client": "I just don't have the energy; it's hard to get started",
                "practitioner": "You sound pretty tired and overwhelmed already",
                "strategy": "Acknowledge current state without reinforcing barrier"
              }
            ]
          },
          "change_talk_reinforcement": {
            "example_sequence": [
              {
                "client": "I used to run before work, it wasn't that bad once I started",
                "practitioner": "So if you did start and kept it up, what would that be like?",
                "strategy": "Build on past success reference to explore change benefits"
              }
            ]
          }
        }
      }
    }
  },
